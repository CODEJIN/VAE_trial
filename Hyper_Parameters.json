{
    "Latent_Size": 2,

    "Encoder": {
        "Label": {
            "Nums": 10,
            "Embedding": 30
        },
        "Hidden1": {
            "Size": 128,
            "Dropout_Rate": 0.2
        },
        "Hidden2": {
            "Size": 128,
            "Dropout_Rate": 0.2
        }
    },
    "Decoder": {
        "Hidden1": {
            "Size": 128,
            "Dropout_Rate": 0.2
        },
        "Hidden2": {
            "Size": 128,
            "Dropout_Rate": 0.2
        }
    },

    "Train": {
        "Batch_Size": 128,
        "Learning_Rate": 1e-3,
        "Max_Pattern_Queue": 50,
        "ADAM": {
            "Beta1": 0.9,
            "Beta2": 0.999,
            "Epsilon": 1e-7
        },

        "Inference_Timing": 1000,
        "Checkpoint_Save_Timing": 1000,

        "Initial_Inference": true
    },

    "Use_Mixed_Precision": false,
    "Inference_Path": "./Results/Inference",
    "Checkpoint_Path": "./Results/Checkpoint",
    "Device": "-1"
}